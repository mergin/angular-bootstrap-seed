{
    "name": "angular-bootstrap-seed",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aot": "ng serve --aot",
        "prebuild": "npm run lint",
        "build": "ng build --aot",
        "build:stats": "ng build --aot --stats-json",
        "prebuild:prod": "npm run lint",
        "prebuild:prod:stats": "npm run lint",
        "build:prod": "ng build --prod --aot --subresourceIntegrity",
        "build:prod:stats": "ng build --prod --aot --subresourceIntegrity --stats-json",
        "analyze": "webpack-bundle-analyzer dist/angular-bootstrap-seed/stats.json",
        "clean": "rm package-lock.json; rm -rf dist/; rm -rf node_modules/",
        "clean:install": "npm run clean && npm install",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.2",
        "@angular/common": "~9.1.2",
        "@angular/compiler": "~9.1.2",
        "@angular/core": "~9.1.2",
        "@angular/forms": "~9.1.2",
        "@angular/localize": "^9.1.2",
        "@angular/platform-browser": "~9.1.2",
        "@angular/platform-browser-dynamic": "~9.1.2",
        "@angular/router": "~9.1.2",
        "@ng-bootstrap/ng-bootstrap": "^6.0.3",
        "@ngx-translate/core": "^12.1.2",
        "@ngx-translate/http-loader": "^4.0.0",
        "bootstrap": "^4.4.1",
        "core-js": "^3.6.5",
        "rxjs": "~6.5.5",
        "tslib": "^1.11.1",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.2",
        "@angular/cli": "~9.1.2",
        "@angular/compiler-cli": "~9.1.2",
        "@angular/language-service": "~9.1.2",
        "@types/jasmine": "~3.5.10",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash": "^4.14.150",
        "@types/node": "^13.13.2",
        "codelyzer": "^5.2.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.1",
        "karma": "~5.0.2",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.3",
        "protractor": "~5.4.4",
        "ts-node": "~8.9.0",
        "tslint": "^6.1.1",
        "typescript": "~3.8.3",
        "webpack-bundle-analyzer": "^3.7.0"
    }
}
