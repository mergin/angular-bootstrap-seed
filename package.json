{
    "name": "angular-bootstrap-seed",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:aot": "ng serve --aot",
        "prebuild": "npm run lint",
        "build": "ng build --aot",
        "build:stats": "ng build --aot --stats-json",
        "prebuild:prod": "npm run lint",
        "prebuild:prod:stats": "npm run lint",
        "build:prod": "ng build --prod --aot --subresourceIntegrity",
        "build:prod:stats": "ng build --prod --aot --subresourceIntegrity --stats-json",
        "analyze": "webpack-bundle-analyzer dist/angular-bootstrap-seed/stats.json",
        "clean": "rm package-lock.json; rm -rf dist/ rm -rf node_modules/",
        "clean:install": "npm run clean && npm install",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "@angular/animations": "~10.0.3",
        "@angular/common": "~10.0.3",
        "@angular/compiler": "~10.0.3",
        "@angular/core": "~10.0.3",
        "@angular/forms": "~10.0.3",
        "@angular/localize": "^10.0.3",
        "@angular/platform-browser": "~10.0.3",
        "@angular/platform-browser-dynamic": "~10.0.3",
        "@angular/router": "~10.0.3",
        "@ng-bootstrap/ng-bootstrap": "^7.0.0",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "bootstrap": "^4.5.0",
        "core-js": "^3.6.5",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.1",
        "@angular/cli": "~10.0.1",
        "@angular/compiler-cli": "~10.0.3",
        "@angular/language-service": "~10.0.3",
        "@commitlint/cli": "^9.0.1",
        "@commitlint/config-conventional": "^9.0.1",
        "@types/jasmine": "~3.5.11",
        "@types/jasminewd2": "~2.0.8",
        "@types/lodash": "^4.14.157",
        "@types/node": "^14.0.20",
        "codelyzer": "^6.0.0",
        "husky": "^4.2.5",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.2",
        "karma": "~5.1.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~3.3.1",
        "karma-jasmine-html-reporter": "^1.5.4",
        "protractor": "~7.0.0",
        "ts-node": "~8.10.2",
        "tslint": "^6.1.2",
        "typescript": "~3.9.6",
        "webpack-bundle-analyzer": "^3.8.0"
    }
}
